</RecommendList>\r\n        </div>\r\n      </Scroll>\r\n      {enterLoading ? <Loading></Loading> : null}\r\n    </Content>\r\n  )\r\n}\r\n\r\nconst mapStateToProps = (state) => ({\r\n  // 不要再这里将数据toJS\r\n  // 不然每次diff对比props的时候都是不一样的引用，导致不必要的重渲染（memo浅比较），属于滥用immutable\r\n  bannerList: state.getIn(['recommend', 'bannerList']),\r\n  recommendList: state.getIn(['recommend', 'recommendList']),\r\n  enterLoading: state.getIn(['recommend', 'enterLoading']),\r\n});\r\n\r\nconst mapDispatchToProps = (dispatch) => {\r\n  return {\r\n    getBannerDataDispatch() {\r\n      dispatch(actionTypes.getBannerList());\r\n    },\r\n    getRecommendListDataDispatch() {\r\n      dispatch(actionTypes.getRecommendList());\r\n    }\r\n  }\r\n}\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(React.memo(Recommend));\r\n","C:\\Users\\alice\\Desktop\\react-web-app\\music-app\\src\\application\\Home\\index.js",[],"C:\\Users\\alice\\Desktop\\react-web-app\\music-app\\src\\application\\Singers\\index.js",["165"],"C:\\Users\\alice\\Desktop\\react-web-app\\music-app\\src\\application\\Rank\\index.js",["166"],"C:\\Users\\alice\\Desktop\\react-web-app\\music-app\\src\\application\\Home\\style.js",["167","168"],"import styled from 'styled-components';\r\nimport style from '../../assets/global-style';\r\n\r\nexport const Top = styled.div`\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: space-between;\r\n  padding: 5px 10px;\r\n  background: ${style [\"theme-color\"]};\r\n  &>span {\r\n    line-height: 40px;\r\n    color: #f1f1f1;\r\n    font-size: 20px;\r\n    &.iconfont {\r\n      font-size: 25px;\r\n    }\r\n  }\r\n`\r\n\r\nexport const Tab = styled.div`\r\n  height: 44px;\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: space-around;\r\n  background: ${style [\"theme-color\"]};\r\n  a {\r\n    flex: 1;\r\n    padding: 2px 0;\r\n    font-size: 14px;\r\n    color: #e4e4e4;\r\n    &.selected {\r\n      span {\r\n        padding: 3px 0;\r\n    